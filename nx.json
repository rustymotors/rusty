{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "clean": {
      "dependsOn": ["^clean"]
    },
    "build": {
      "dependsOn": ["clean", "^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "cache": true
    }
  },
  "defaultBase": "main",
  "nxCloudAccessToken": "ODcyNmM1ZTEtNWY2Yi00NDg4LWEzMTYtMDBkYzExMTdjNDgwfHJlYWQtd3JpdGU=",
  "plugins": [
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "testTargetName": "test"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ]
}
